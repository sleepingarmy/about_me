<!DOCTYPE html>
<html>
<head>
  <title>AboutMe</title>
  <%= stylesheet_link_tag    'application', media: 'all'%>
  <%= javascript_include_tag 'application'%>
  <%= csrf_meta_tags %>

  <!-- google fonts  -->
  <link href='http://fonts.googleapis.com/css?family=Poiret+One|Pacifico|Raleway:400,100,200,300,500,600,700' rel='stylesheet' type='text/css'>

  <!-- fontawesome -->
  <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css">

</head>
<body>
  <div class="container">
    <header class="nav navbar-static-top">
      <ul class="nav navbar-nav">
        <li class="text-center"><%= link_to("home", welcome_index_path) %></li>
        <li class="text-center"><%= link_to("about", about_index_path) %></li>
        <li class="text-center"><%= link_to("contact", contact_index_path) %></li>
      </ul>
    </header>
</div>
<%= yield %>

</body>
</html>





